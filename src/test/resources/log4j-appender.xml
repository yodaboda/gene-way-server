<?xml version="1.0" encoding="UTF-8"?>

<Configuration status="DEBUG" name="TestCustomLevels">
  <Appenders>
    <Console name="STDOUT">
      <PatternLayout pattern="%m%n"/>
    </Console>
    <List name="List"/>
  </Appenders>

  <Loggers>
    <Root level="DEBUG">
      <AppenderRef ref="STDOUT"/>
      <AppenderRef ref="List"/>
    </Root>
  </Loggers>

</Configuration>